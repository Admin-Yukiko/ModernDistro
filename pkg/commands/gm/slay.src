use os;
use uo;
use boat;
include ":attributes:attributes";

program textcmd_kill( who )

	var mob := Target( who, TGTOPT_NOCHECK_LOS );
	if (mob)

		if((mob.cmdlevel) && mob.cmdlevel > who.cmdlevel)
			RevokePrivilege( who, "invul" );
			ApplyRawDamage( who, CInt(AP_GetStat(mob, "Strength")) + 100 );
			return;
		endif

		if (!mob.dead)
			RevokePrivilege( mob, "invul" );
			ApplyRawDamage( mob, CInt(AP_GetStat(mob, "Strength")) + 100 );
		endif

	endif

endprogram