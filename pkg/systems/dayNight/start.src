use os;
use polsys;

var realm_list := Realms().Keys();
foreach realm in ( realm_list )
	var script := Start_Script("dayNight", realm);
	if ( script.errortext )
		SysLog(script.errortext);
	else
		Print("Starting day night controller for "+realm+"... OK.");
	endif
endforeach

return 1;
