use uo;
use os;

program TextCommand(user, text)
	var pid := GetGlobalProperty("#DayNightPid-"+user.realm);
	var process := GetProcess(pid);
	
	SendSysMessage(user, "Process->"+process);
	
	var result := Process.SendEvent(text);
	SendSysMessage(user, "Result="+result);
	
	return 1;
endprogram