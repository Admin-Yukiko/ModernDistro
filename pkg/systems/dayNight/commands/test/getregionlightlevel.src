use uo;

program TextCommand(user)
	var region_name := GetRegionNameAtLocation(user.x, user.y, user.realm);
	SendSysmessage(user, "Region name = "+region_name);
	
	var level := GetRegionLightLevelAtLocation(user.x, user.y, user.realm);
	SendSysMessage(user, "LightLevel Should be "+level);
	
	return 1;
endprogram
