use uo;
use os;
use math;

include ":daynight:dayNight";

program TextCommand(user)
	SendSysMessage(user, "----------", 3, 32);
	SendSysMessage(user, "RL SecondsInFullDay->"+GetRLSecondsInFullDay());
	SendSysMessage(user, "RL MinutesInFullDay->"+GetRLMinutesInFullDay());
	SendSysMessage(user, "RL HoursInFullDay->"+GetRLHoursInFullDay());
	SendSysMessage(user, "Game Time->"+GetGameTime());
	SendSysMessage(user, "Game Hour->"+GetGameHour());
	SendSysMessage(user, "Game Minute->"+GetGameMinute());
	SendSysMessage(user, "----------", 3, 32);
	Unload_Scripts("timetest");	
	return 1;
endprogram
