use uo;
use os;

include ":attributes:attributes";

program TimedScript_EndAgility(params)
	var mobile := params[1];
	
	var points := CInt(GetObjProperty(mobile, "BlessMod"));	
	AP_ModifyStatMod(mobile, DEXTERITY, -1 * points);
	AP_ModifyStatMod(mobile, INTELLIGENCE, -1 * points);
	AP_ModifyStatMod(mobile, STRENGTH, -1 * points);
	EraseObjProperty(mobile, "BlessMod");
	
	return 1;
endprogram

